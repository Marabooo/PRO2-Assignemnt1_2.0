    <Button fx:id="unmarkforRemovalButton"  layoutX="14.0" layoutY="363.0"  onAction="#unmarkForRemoval" mnemonicParsing="false" prefHeight="54.0" prefWidth="110.0" text="Unmark for Removal" textAlignment="CENTER" wrapText="true" />
    <opaqueInsets>
