<AnchorPane id="State" prefHeight="442.0" prefWidth="619.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.VinylViewController">

    <TableView fx:id="vinylTable" layoutX="129.0" layoutY="14.0" prefHeight="413.0" prefWidth="476.0">
