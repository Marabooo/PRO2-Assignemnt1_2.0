            <TableColumn fx:id="titleColumn" prefWidth="147.60000610351562" text="Title" />
            <TableColumn fx:id="artistColumn" prefWidth="122.0" text="Artist" />
            <TableColumn fx:id="releaseYearColumn" prefWidth="91.5999755859375" text="Release Year" />
            <TableColumn fx:id="stateColumn" prefWidth="116.20004272460938" text="State" />
